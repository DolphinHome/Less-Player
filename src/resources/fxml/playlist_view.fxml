<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>

<BorderPane fx:id="playlist_view" xmlns:fx="http://javafx.com/fxml/1">
	<top>
		<AnchorPane fx:id="playlist_top">
			<children>
				<Label fx:id="logo_size" />
				<HBox fx:id="win_btns_box" >
					<children>
						<Label fx:id="attach_btn" />
						<Label fx:id="target_btn" />
						<Label fx:id="close_btn" />
					</children>
				</HBox>
			</children>
		</AnchorPane>
	</top>
	<center>
		<ListView fx:id="list_view" />
	</center>
</BorderPane>

